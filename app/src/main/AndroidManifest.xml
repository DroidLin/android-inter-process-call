<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".TestApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Androidinterprocesscall"
        tools:targetApi="31">
        <activity
            android:name=".LibraryActivity"
            android:exported="false"
            android:process=":library" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".LibraryProcessBroadcastReceiver"
            android:exported="false"
            android:process=":library">
            <intent-filter>
                <action android:name="io.lza.process.connection.broadcast.receiver" />
            </intent-filter>
        </receiver>

        <receiver
            android:name=".MainProcessBroadcastReceiver"
            android:exported="false">
            <intent-filter>
                <action android:name="io.lza.process.connection.broadcast.main" />
            </intent-filter>
        </receiver>
    </application>

</manifest>